
# List manipulation 
define mylen (n) {
    if empty (n) {
        0
    } else {
        1 + mylen (rest (n))
    }
}

# mylen ([1, 3, 3, 5, 6, 7])

# Closures 
define adder (a) {
    fun (b) { 
        a + b
    }
}
define add10 adder (10)
define result add10 (10)



# Tail rec 
define sum (acc, start, finish) {
    if start > finish {
        acc
    } else { 
        sum (acc+start, start+1, finish)
    }
}

sum (0, 1, 1000000)